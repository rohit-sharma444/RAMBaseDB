{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Workspace": {
    "DefaultDatabaseName": "Workspace",
    "SnapshotDirectory": "Snapshots",
    "ConfigurationDirectory": "Config",
    "MetadataDirectory": "Metadata",
    "UsersDirectory": "Users",
    "DumpFilePrefix": "workspace",
    "SnapshotInterval": "00:01:00",
    "MaxSnapshotHistory": 25,
    "EnableAutomaticSnapshots": true,
    "AutoRestoreLatestDump": true,
    "BootstrapMetadata": true,
    "InsightSnapshotDepth": 6
  },
  "AllowedHosts": "*"
}
